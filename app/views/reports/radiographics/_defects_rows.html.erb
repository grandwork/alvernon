<thead>
	<tr>
		<td class="shaded">Part Number</td>
		<td class="shaded">Welder</td>
		<td class="shaded">Area</td>
		<td class="shaded">Films</td>
		<td class="shaded">Result</td>
		<td class="shaded">Defect Type</td>
		<td class="shaded">Length</td>
		<td class="shaded">Location</td>
	</tr>
</thead>
<% rt.rt_defects.each do |defect| %>
	<tr class="<%= cycle("odd", "even", :name => "row_class") %> fields">
		<td><%= defect.part_number %></td>
		<td><%= defect.welder %></td>
		<td><%= defect.area %></td>
		<td><%= defect.films %></td>
		<td class="<%= accept_or_reject_class(defect) %>"><%= defect.accept_reject %></td>
		<td><%= defect.defect_type %></td>
		<td><%= defect.length %></td>
		<td><%= defect.location %></td>
	</tr>
<% end %>