<thead>
	<tr>
		<td class="shaded">ID</td>
		<td class="shaded">Start</td>
		<td class="shaded">Length</td>
		<td class="shaded">Depth</td>
		<td class="shaded">Height</td>
		<td class="shaded">Angle</td>
		<td class="shaded">dB</td>
		<td class="shaded">Type</td>
		<td class="shaded">Result</td>
	</tr>
</thead>
<% ut.ut_defects.each do |defect| %>
	<tr class="<%= cycle("odd", "even", :name => "row_class") %> fields">
		<td><%= defect.defect_id %></td>
		<td><%= defect.position %></td>
		<td><%= defect.length %></td>
		<td><%= defect.depth %></td>
		<td><%= defect.height %></td>
		<td><%= defect.angle %></td>
		<td><%= defect.db %></td>
		<td><%= defect.defect_type %></td>
		<td class="<%= accept_or_reject_class(defect) %>"><%= defect.result %></td>
	</tr>
<% end %>