    </tr>
    
    <tr>
      <td class="shaded">Part No.?</td>
      <td></td>
      <td class="shaded">Client No.?</td>
      <td></td>
    </tr>
      
    <tr>
      <td class="shaded">Procedure</td>
      <td><%= report.procedure %></td>
      
      <td class="shaded">Material?</td>
      <td></td>
    </tr>
    
    <tr>
      <td class="shaded">Acceptance</td>
      <td><%= report.code %></td>
      <td class="shaded">Surface?</td>
      <td></td>
    </tr>
    
