    <tr>
      <td class="shaded">Project</td>
      <td><%= report.title %></td>

      <td class="shaded" id="col3">Date</td>
      <td id="col4"><%= report.date.strftime("%d-%m-%Y") %></td>
    </tr>

    <tr>
      <td class="shaded" id="col1">Client</td>
      <td id="col2"><%= report.client %></td>
      
